{
  "schemaVersion": 1,
  "id": "chromium",
  "version": "${version}",

  "name": "Chromium",
  "description": "Another fabric mod with some functional",
  "authors": [
    "DenaryDev"
  ],
  "contact": {
    "homepage": "https://github.com/SapphireMC/Chromium-Fabric",
    "sources": "https://github.com/SapphireMC/Chromium-Fabric.git",
    "issues": "https://github.com/SapphireMC/Chromium-Fabric/issues"
  },
  "license": "MIT",
  "icon": "assets/chromium/icon.png",

  "environment": "client",
  "entrypoints": {
    "main": [
      "me.denarydev.chromium.ChromiumMod"
    ],
    "client": [
      "me.denarydev.chromium.client.ChromiumClientMod"
    ],
    "modmenu": [
      "me.denarydev.chromium.client.compat.ModMenuCompat"
    ]
  },
  "mixins": [
    "mixins.chromium.json",
    "mixins.chromium.compat.sodium.json",
    "mixins.chromium.compat.iris.json"
  ],
  "accessWidener": "chromium.accesswidener",

  "depends": {
    "fabricloader": ">=0.15.11",
    "minecraft": "1.21",
    "java": "21",
    "fabric-api": ">=0.100.4",
    "cloth-config": "15.0.x"
  },
  "breaks": {
    "rrls": "*"
  }
}
